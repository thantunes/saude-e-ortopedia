{
  "flex-layout.row#mega-menu": {
    "props": {
      "blockClass": "mega-menu",
      "colSizing": "auto",
      "fullWidth": false,
      "horizontalAlign": "left",
      "preserveLayoutOnMobile": false,
      "preventHorizontalStretch": false,
      "preventVerticalStretch": false
    },
    "children": [
      "flex-layout.col#extra-menu-desktop"
    ]
  },
  "flex-layout.col#extra-menu-desktop": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": ["vtex.menu@2.x:menu#extra-menu"]
  },
  "vtex.menu@2.x:menu#extra-menu": {
    "props": {
      "experimentalOptimizeRendering": true,
      "blockClass":"main-menu"
    },
    "children": [
      "menu-item#mobilidade",
      "menu-item#conforto-do-paciente",
      "menu-item#almofadas-e-colchoes",
      "menu-item#travesseiros-e-capas",
      "menu-item#produtos-ortopedicos",
      "menu-item#meias-de-compressao",
      "menu-item#cuidado-pes-e-corpo",
