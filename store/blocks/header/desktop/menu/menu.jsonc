      "menu-item#aparelhos"
    ]
  },
  "menu-item#mobilidade": {
    "props": {
      "id": "mobilidade",

      "itemProps": {
        "text": "Mobilidade e Acessórios",
        "tagTitle": "Mobilidade e Acessórios",
        "href": "/mobilidade-e-acessorios"
      },
      "iconProps": {
        "id": "mobilidade-e-acessorios",
        "size": 34,
        "viewBox": "0 0 20.97 34.706",
        "activeClassName": "rebel-pink",
        "mutedClassName": "c-action-primary"
      }
    },
    "blocks": [
